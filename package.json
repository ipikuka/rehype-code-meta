{
  "name": "rehype-code-meta",
  "version": "1.0.4",
  "description": "Rehype plugin to copy 'code.data.meta' to 'code.properties.metastring'",
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc --build && type-coverage",
    "format": "npm run prettier && npm run lint",
    "prettier": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest --watch=false",
    "test:watch": "vitest",
    "test:file": "vitest test.spec.ts",
    "prepack": "npm run build",
    "prepublishOnly": "npm run test && npm run format && npm run test-coverage",
    "test-coverage": "vitest run --coverage"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipikuka/rehype-code-meta.git"
  },
  "keywords": [
    "unified",
    "rehype",
    "hast",
    "markdown",
    "mdx",
    "plugin",
    "rehype plugin",
    "rehype code",
    "code meta",
    "metastring",
    "rehype code meta"
  ],
  "author": "ipikuka <talatkuyuk@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ipikuka/rehype-code-meta#readme",
  "bugs": {
    "url": "https://github.com/ipikuka/rehype-code-meta/issues"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/dedent": "^0.7.2",
    "@types/node": "^24.10.13",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.9",
    "dedent": "^1.7.1",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "rehype-highlight-code-lines": "^1.2.2",
    "rehype-parse": "^9.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "rimraf": "^6.1.3",
    "type-coverage": "^2.29.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "unified": "^11.0.5",
    "vfile": "^6.0.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@types/hast": "^3.0.4",
    "unist-util-visit": "^5.1.0"
  },
  "peerDependencies": {
    "unified": "^11"
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "ignoreAsAssertion": true,
    "strict": true
  },
  "sideEffects": false
}
